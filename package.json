{
  "name": "site-name",
  "scripts": {
    "start": "rm -rf _site && npx @11ty/eleventy --serve --incremental",
    "build": "rm -rf _site && npx @11ty/eleventy",
    "node_modules": "rm -rf node_modules && sudo npm install"
  },
  "dependencies": {
    "@dh253/nds": "^1.1.1"
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "arrowParens": "always",
    "endOfLine": "lf"
  }
}
